- id: \citationkey
  type: \entrytype
  title: "\format[RemoveBrackets && FormatChars]{\title}"
  \begin{booktitle}proceedings: "\format[FormatChars]{\booktitle}"\end{booktitle}
  \begin{journal}journal: "\format[FormatChars]{\journal}"\end{journal}
  \begin{eventtitle}event: "\format[FormatChars]{\eventtitle}"\end{eventtitle}
  \begin{school}school: "\format[FormatChars]{\school}"\end{school}
  authors: "\format[RemoveBrackets && FormatChars]{\author}"
  place: "\address"
  url_slug: \format[ToLowerCase]{\note}\year
  \begin{eventdate}date: "\eventdate"\end{eventdate}
  issued:
  - year: \year
    month: "\month"
